{
  "name": "pl-visa",
  "description": "For poland visa",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "npm run clean && npm run lint && npm run manifest && npm run build",
    "build": "rollup --config",
    "manifest": "node manifest.js",
    "lint": "eslint --ext \".js\" --ignore-path .gitignore . --max-warnings=0",
    "clean": "rimraf dist/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "axios": "^0.26.1",
    "eslint": "^8.13.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2"
  },
  "eslintConfig": {
    "env": {
      "es2022": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "unicorn"
    ],
    "extends": "plugin:unicorn/all",
    "ignorePatterns": [
      "dist/",
      "rollup.config.js"
    ]
  },
  "dependencies": {}
}
